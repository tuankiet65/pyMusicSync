{
  "threadNum": 3,
  "syncSource": [
    "/media/Entertainment"
  ],
  "syncDestination": "/media/Data1/pyMusicSync/testEnv/dst",
  "dryRun": false,
  "encoderSetting": {
    "codec": "ogg",
    "bitrateControl": "vbr",
    "quality": "7"
  },
  "autosaveInterval": 1,
  "filter": [
    {
      "field": "duration",
      "operator": "<=",
      "value": 900
    },
    {
      "field": "album",
      "operator": "regex",
      "value": "/(EDM\.com Exclusives|Unknown Title|None|All Covers|Unknown Title\u0000|Solutions Advanced Workbook CD|CAE Results|mp3.zing.vn|FCE Practice Tests)/"
      "applyNot": true
    }
    {
      "field": "album",
      "operator": "==",
      "value": "",
      "applyNot": true
    }
  ],
  "modifiers": [
    {
      "field": "album",
      "match": "BAYMETAL",
      "replaceWith": "Babymetal"
    }
  ]
}
